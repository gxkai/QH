  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --exec babel-node",
    "build": "babel server -d es5-server&&nuxt build&&rimraf dist&& cpx \"{nuxt.config.js,es5-server/**,.nuxt/**,.env,.env.development,.env.production,package.json,server.json,Dockerfile}\" dist",
